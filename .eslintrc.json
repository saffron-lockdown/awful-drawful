{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "import/order": "off",
    "no-console": "off",
    "no-underscore-dangle": "off"
  },
  "plugins": ["jest"]
}
